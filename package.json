    "clsx": "^2.1.1",
    "isbot": "^5.1.27",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.62.0",
    "react-router": "^7.9.5",
    "tailwind-merge": "^3.3.1",
